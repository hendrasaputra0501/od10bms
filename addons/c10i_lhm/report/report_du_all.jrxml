	<queryString>
		<![CDATA[SELECT     
hf.code as kemandoran
,he.name_related as employee_name
,he.no_induk 
,hp.code as ptkp_code
,het.code as employee_type
,hd.code as division
,hke,hkne
,total_hk
,hke_rp
,hkne_rp
,total_hk_rp
,psdu.basic_salary
,tunjangan_lain
,premi_lembur
,natura
,rapel
,tunj_bpjs_pensiun
,tunj_bpjs_kesehatan
,tunj_bpjs_tk
,tunj_lebih_hari
,gaji_bruto
,pot_bpjs_pensiun
,pot_bpjs_kesehatan
,pot_bpjs_tk
,pot_lain
,pot_kurang_hari
,pph_21
,total_potongan
,upah_diterima
,from_date
,to_date
,rc.name as company_name
FROM plantation_salary_daftar_upah psdu 
left join plantation_salary ps on ps.id=psdu.salary_id
left join hr_employee he on he.id=psdu.employee_id
left join hr_foreman hf on hf.id=psdu.kemandoran_id
left join hr_ptkp hp on hp.id=psdu.ptkp_id
left join hr_employee_type het on het.id=psdu.employee_type_id
left join hr_division hd on hd.id=psdu.division_id
left join res_company rc on rc.id=ps.company_id
where salary_id= $P{id} and  $P!{type_query} 
order by hf.code,he.no_induk]]>
	</queryString>
