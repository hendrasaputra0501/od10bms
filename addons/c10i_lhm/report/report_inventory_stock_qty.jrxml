	<queryString>
		<![CDATA[select 
rc.name AS company_name
,ou."name" as operating_unit
,pt.default_code AS product_code
,pt."name" AS product_name
,pu.name AS uom_name
,SUM(CASE WHEN grp ='beg_bal' THEN db_data."qty" ELSE 0 END) AS beg_bal
,SUM(CASE WHEN grp ='beg_bal' THEN db_data."qty_rp" ELSE 0 END) AS beg_bal_rp
,SUM(CASE WHEN grp ='dr' THEN db_data."qty" ELSE 0 END) AS dr
,SUM(CASE WHEN grp ='dr' THEN db_data."qty_rp" ELSE 0 END) AS dr_rp
,SUM(CASE WHEN grp ='cr' THEN -db_data."qty" ELSE 0 END) AS cr
,SUM(CASE WHEN grp ='cr' THEN -db_data."qty_rp" ELSE 0 END) AS cr_rp
,SUM(CASE WHEN grp ='adj' THEN db_data."qty" ELSE 0 END) AS adj
,SUM(CASE WHEN grp ='adj' THEN db_data."qty_rp" ELSE 0 END) AS adj_rp
,SUM(db_data."qty") AS end_bal
,SUM(db_data."qty_rp") AS end_bal_rp
FROM
(
SELECT 'beg_bal' AS grp,aml.company_id,account_id,product_id,aml.operating_unit_id
,sum(CASE WHEN balance < 0 THEN -quantity ELSE quantity END)qty
,sum(balance)qty_rp
FROM account_move_line aml
LEFT JOIN account_move am ON am.id=aml.move_id
WHERE am."date" < $P{from_date}  ::date
AND $P!{operating_unit_ids_query}
GROUP BY aml.company_id,aml.operating_unit_id,product_id,account_id
UNION ALL
SELECT 'dr' AS grp,aml.company_id,account_id,product_id,aml.operating_unit_id
	,sum(quantity)qty,sum(balance)qty_rp
FROM account_move_line aml
LEFT JOIN account_move am ON am.id=aml.move_id
WHERE am."date" BETWEEN $P{from_date}  ::date AND $P{to_date}  ::date 
AND debit>0
AND am.journal_id not in (select id from account_journal where code ='AJE')
AND $P!{operating_unit_ids_query}
GROUP BY aml.company_id,aml.operating_unit_id,product_id,account_id
UNION ALL
SELECT 'cr' AS grp,aml.company_id,account_id,product_id,aml.operating_unit_id
	,sum(-quantity)qty,sum(balance)qty_rp
FROM account_move_line aml
LEFT JOIN account_move am ON am.id=aml.move_id
WHERE am."date" BETWEEN $P{from_date}  ::date AND $P{to_date}  ::date 
AND credit>0
AND am.journal_id not in (select id from account_journal where code ='AJE')
AND $P!{operating_unit_ids_query}
GROUP BY aml.company_id,aml.operating_unit_id,product_id,account_id
UNION ALL
SELECT 'adj' AS grp,aml.company_id,account_id,product_id,aml.operating_unit_id
	,COALESCE(sum(CASE WHEN balance < 0 THEN -quantity ELSE quantity END),0)qty,sum(balance)qty_rp
FROM account_move_line aml
LEFT JOIN account_move am ON am.id=aml.move_id
WHERE am."date" BETWEEN $P{from_date}  ::date AND $P{to_date}  ::date 
AND am.journal_id in (select id from account_journal where code ='AJE')
AND $P!{operating_unit_ids_query}
GROUP BY aml.company_id,aml.operating_unit_id,product_id,account_id
) db_data
LEFT JOIN res_company rc on rc.id=db_data.company_id
LEFT JOIN operating_unit ou ON ou.id=db_data.operating_unit_id
LEFT JOIN account_account aa ON aa.id=db_data.account_id
LEFT JOIN product_product pp ON pp .id=db_data.product_id
LEFT JOIN product_template pt ON pt .id=pp.product_tmpl_id
LEFT JOIN product_uom pu ON pu .id=pt.uom_id
WHERE account_id IN
(
SELECT DISTINCT CAST(REPLACE(value_reference,'account.account,','') AS INT) as account_id FROM ir_property WHERE res_id in
(SELECT distinct concat('product.category',',',pc.id) as categ
FROM stock_move sm 
LEFT JOIN product_product pp ON pp.id=sm.product_id
LEFT JOIN product_template pt ON pt.id=pp.product_tmpl_id
LEFT JOIN product_category pc ON pc.id=pt.categ_id
)
AND name='property_stock_valuation_account_id'
)

GROUP BY rc.name,ou."name",pt.default_code,pt."name",pu.name
ORDER BY 1,2,3]]>
	</queryString>
