	<queryString language="SQL">
		<![CDATA[select 
ail.id as line_id
,ai."number"
,rp.name as cust_name
,rp.street street
,rp.street2 street2
, ((COALESCE(rp.city,'')) || (COALESCE(', '||rp.zip,''))) AS city
,rp.npwp_number as npwp

,rps.name as cust_name_ship
,rps.street street_ship
,rps.street2 street_ship2
, ((COALESCE(rps.city,''))||
	 (COALESCE(', '||rps.zip,'')))city_ship

,ai.date_invoice
,ai.name as Cust_PO
,ai.origin as Cust_SO
,apt.name as payment_term
,ai.amount_untaxed as dpp
, ai.amount_tax tax
, ai.amount_total as total
, pt.default_code as default_code
, trim(substr(ail."name", position(']' in ail."name")+1)) prod_name
,ail.quantity
,ail.price_unit*((100-discount)/100) price_unit
,ail.price_subtotal
,rcur.name as mata_uang
,rcur.symbol as symbol
,ai.comment
,apt.name payment_term
,pu.name AS satuan   
,ai.date_due as due_date
,ai.name as ref
,rcy.id AS company_id
,ai.id AS invoice_id
from account_invoice ai
left join account_invoice_line ail on ai.id=ail.invoice_id
LEFT JOIN res_currency rcur ON rcur.id = ai.currency_id
LEFT JOIN res_partner rp ON ai.partner_id = rp.id
LEFT JOIN res_partner rps ON ai.partner_shipping_id = rps.id
LEFT JOIN res_country rc ON rc."id" = rp.country_id
LEFT JOIN res_country_state rcs ON rcs."id" = rp.state_id

LEFT JOIN product_uom pu ON ail.uom_id = pu.id
LEFT JOIN product_product pp ON pp.id = ail.product_id
LEFT JOIN product_template pt ON pt.id = pp.product_tmpl_id
LEFT JOIN account_payment_term apt ON apt.id = ai.payment_term_id
LEFT OUTER JOIN res_company rcy ON rcy.id = ai.company_id
WHERE ai.id= $P{id}

union all

SELECT  generate_series(
(select id from account_invoice_line where invoice_id = $P{id}  order by id desc limit 1)+1
,((select id from account_invoice_line where invoice_id = $P{id}  order by id desc limit 1) + 23) - (select count(*) from account_invoice_line where invoice_id = $P{id} )) as line_id
, NULL as number
, NULL as cust_name
, NULL as street
, NULL as street2
, NULL as city
, NULL as npwp

, NULL  as cust_name_ship
, NULL  as street_ship
, NULL  as street_ship2
,  NULL  as city_ship

, NULL as date_invoice
, NULL as Cust_PO
, NULL as Cust_SO
, NULL as payment_term
,(SELECT round(amount_untaxed) FROM account_invoice WHERE "id" = $P{id}) dpp
, (SELECT round(amount_tax) FROM account_invoice WHERE "id" =$P{id}) AS tax
, (SELECT round(amount_total) FROM account_invoice WHERE "id" = $P{id})  AS total
, NULL as default_code
, NULL as prod_name
, NULL as quantity
, NULL as price_unit
 , NULL as price_subtotal
, (SELECT curr.name FROM account_invoice ai
	LEFT  JOIN res_currency curr ON curr."id" = ai.currency_id
	WHERE ai."id" = $P{id} ) as mata_uang
, (SELECT curr.symbol FROM account_invoice ai
	LEFT  JOIN res_currency curr ON curr."id" = ai.currency_id
	WHERE ai."id" = $P{id} ) as symbol
,(select comment from account_invoice where id= $P{id}) as comment
, NULL as payment_term
, NULL as satuan 
, NULL  as due_date
,NULL as Ref
, NULL AS company_id
, (SELECT "id" FROM account_invoice ai WHERE ai."id" = $P{id}) AS invoice_id
order by  1]]>
	</queryString>
