	<parameter name="id" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[select 
case when av.voucher_type='sale' then 'PENERIMAAN' else 'PEMBAYARAN' end as voucher_type
,av.number as voucher_no
,av.date as voucher_tgl
,av.name as reference
,case when aj.type ='bank' then 'Bank' when aj.type ='cash' then 'Cash' end as cash_bank_type
,aj.name as cash_bank_name
,COALESCE(rp.name,'') as partner_name
,llt.code as type
,ll.code as location
,la.code as activity_code
,la.name as activity_name
,trim(avl.name) as deskripsi
,avl.price_subtotal
, rc.name AS company_name
, rcp.street AS street
, rcp.street2 AS street2
, rcp.city AS city
, rcs.name AS state_name
, rcp.zip AS zip_code
from account_voucher av
left join account_voucher_line avl on av.id=avl.voucher_id
left join account_journal aj on aj.id = av.journal_id
left join res_partner rp on rp.id=av.partner_id
left join account_account aa on aa.id=avl.account_id
left join lhm_location_type llt on llt.id = avl.plantation_location_type_id
left join lhm_location ll on ll.id = avl.plantation_location_id
left join lhm_activity la on la.id = avl.plantation_activity_id
LEFT OUTER JOIN res_company rc ON rc.id = aj.company_id
LEFT OUTER JOIN res_partner rcp ON rcp.id = rc.partner_id
LEFT OUTER JOIN res_country_state rcs On rcs.id = rcp.state_id
WHERE av."id" = $P{id}
order by avl.id]]>
	</queryString>
