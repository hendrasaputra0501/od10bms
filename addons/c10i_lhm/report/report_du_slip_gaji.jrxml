	<queryString language="SQL">
		<![CDATA[SELECT     
ap.name as period
,he.name_related 
,he.no_induk
,het.code as employee_type
,total_hk_rp
,basic_salary
,(tunj_lebih_hari-pot_kurang_hari) as lebih_kurang
,premi_lembur
,natura
,tunjangan_lain
,rapel
,tunj_bpjs_pensiun
,tunj_bpjs_kesehatan
,tunj_bpjs_tk
,basic_salary+(tunj_lebih_hari-pot_kurang_hari)+premi_lembur+natura+tunjangan_lain+rapel+tunj_bpjs_pensiun+tunj_bpjs_kesehatan+tunj_bpjs_tk
as gabruto
,pot_bpjs_pensiun
,pot_bpjs_kesehatan
,pot_bpjs_tk
,pot_lain
,pph_21
,(pot_bpjs_pensiun+pot_bpjs_kesehatan+pot_bpjs_tk+pot_lain+pph_21)total_potongan
,upah_diterima
,from_date
,to_date
,rc.name as company_name
FROM plantation_salary_daftar_upah psdu 
left join plantation_salary ps on ps.id=psdu.salary_id
left join account_period ap on ap.id=ps.period_id 
left join hr_employee he on he.id=psdu.employee_id
left join hr_foreman hf on hf.id=psdu.kemandoran_id
left join hr_employee_type het on het.id=psdu.employee_type_id
left join res_company rc on rc.id=ps.company_id
where salary_id= $P{id} and  $P!{type_query} 
order by hf.code,he.no_induk]]>
	</queryString>
