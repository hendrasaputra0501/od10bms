	<queryString>
		<![CDATA[SELECT
            lt.name AS "ID",
            lt.date AS "TANGGAL TRANSAKSI",
            hf.code AS "KODE KEMANDORAN",
            hf."name" AS "NAMA KEMANDORAN",
            he_mdr1.no_induk AS "MANDOR 1",
            he_mdr1.name_related AS "NAMA MANDOR 1",
            he_mdr.no_induk AS "MANDOR",
            he_mdr.name_related AS "NAMA MANDOR",
            he_krn.no_induk AS "KERANI",
            he_krn.name_related AS "NAMA KERANI",
            ('') AS "MANDOR TRANSPORT",
            ('') AS "NAMA MANDOR TRANSPORT",
            ('') AS "MANDOR 1 TRANSPORT",
            ('') AS "NAMA MANDOR 1 TRANSPORT",
            he.no_induk AS "NIK",
            he.name_related as "NAMA KARYAWAN",
            hat.code AS "ABSENSI",
            llt.code AS "TYPE LOKASI",
            ll.code AS "KODE LOKASI",
            ll.name AS "NAMA LOKASI",
            la.code AS "KODE AKTIVITAS",
            la.name AS "NAMA AKTIVITAS",
            ltl.work_day AS "HK",
            pu.name AS "UNIT",
            ltl.work_result AS "VOLUME",
            ltl.unit_price AS "TARIF_PER_SATUAN",
            ltl.premi AS "PREMI",
            ltl.overtime_value AS "LEMBUR",
            ltl.penalty AS "PENALTI",
            lt.note AS "NOTE",
            ltl.create_date AS "INPUT_DATE",
            rp_cr.name AS "INPUT_BY",
            ltl.write_date AS "UPDATE_DATE",
            rp_up.name AS "UPDATE_BY",
            rc.name AS "COMPANY_CODE"
            FROM lhm_transaction lt
                LEFT JOIN lhm_transaction_line ltl ON lt.id = ltl.lhm_id
                 LEFT JOIN hr_foreman hf ON lt.kemandoran_id = hf.id
                 LEFT JOIN hr_employee he_mdr ON hf.foreman_id = he_mdr.id
                 LEFT JOIN hr_employee he_mdr1 ON hf.foreman_id_1 = he_mdr1.id
                 LEFT JOIN hr_employee he_krn ON hf.admin_id = he_krn.id
                 LEFT JOIN hr_employee he_kry ON ltl.employee_id = he_kry.id
                 LEFT JOIN hr_employee he ON ltl.employee_id = he.id
                 LEFT JOIN hr_attendance_type hat ON ltl.attendance_id = hat.id
                 LEFT JOIN lhm_location_type llt ON ltl.location_type_id = llt.id
                 LEFT JOIN lhm_location ll ON ltl.location_id = ll.id
                 LEFT JOIN lhm_activity la ON ltl.activity_id = la.id
                 LEFT JOIN product_uom pu ON ltl.satuan_id = pu.id
                 LEFT JOIN res_users ru_cr ON lt.create_uid = ru_cr.id
                 LEFT JOIN res_partner rp_cr ON ru_cr.partner_id = rp_cr.id
                 LEFT JOIN res_users ru_up ON lt.write_uid = ru_up.id
                 LEFT JOIN res_partner rp_up ON ru_cr.partner_id = rp_up.id
                 LEFT JOIN res_company rc ON rc.id = lt.company_id
              WHERE lt.company_id =  $P{company_id} 
              AND lt.date::date  between  $P{from_date}::date and  $P{to_date}::date  and lt.state in ('done','in_progress','close') and hat.code is not null
              ORDER BY hf.code,ltl.date, he.no_induk]]>
	</queryString>
