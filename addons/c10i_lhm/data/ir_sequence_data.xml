<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record model="ir.sequence" id="seq_nab_input">
            <field name="name">Plantation NAB Sequence</field>
            <field name="code">lhm.nab</field>
            <field name="prefix">NAB-%(y)s%(month)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
            <field name="implementation">no_gap</field>
        </record>

        <record model="ir.sequence" id="seq_nab_afkir_input">
            <field name="name">Plantation NAB Afkir Sequence</field>
            <field name="code">lhm.nab.afkir</field>
            <field name="prefix">NAF-%(y)s%(month)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
            <field name="implementation">no_gap</field>
        </record>

        <record model="ir.sequence" id="seq_plantation_salary">
            <field name="name">Plantation Salary Sequence</field>
            <field name="code">plantation.salary</field>
            <field name="prefix">DU-%(y)s%(month)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
            <field name="implementation">no_gap</field>
        </record>

        <record model="ir.sequence" id="seq_plantation_allowance">
            <field name="name">Plantation Allowance Sequence</field>
            <field name="code">plantation.allowance.other</field>
            <field name="prefix">PDT-%(y)s%(month)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
            <field name="implementation">no_gap</field>
        </record>

    </data>
</odoo>