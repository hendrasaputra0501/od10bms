	<queryString>
		<![CDATA[SELECT item.id
    , item.default_code
    , itemtmpl.name AS itemname
    , sub.opening_qty AS init_qty
    , sub.opening_value AS init_qty_cost
    , sub.closing_qty AS ending_qty
    , sub.closing_value AS ending_qty_cost
    , sub.incoming_qty AS balancein
    , sub.incoming_value AS balancein_cost
    , sub.outgoing_qty AS balanceout
    , sub.outgoing_value AS balanceout_cost
    , sub.adjustment_in_qty AS adjust_in
    , sub.adjustment_out_qty adjust_out
    , (sub.adjustment_in_qty - sub.adjustment_out_qty) AS adjustment
    , (sub.adjustment_in_value - sub.adjustment_out_value) AS adjustment_cost
    , uom.name uomname
FROM 
    (SELECT
        pp.id as product_id, 
        coalesce(opening_query.qty, 0.0) as opening_qty,
        coalesce(vopening_query.value, 0.0) as opening_value,
        coalesce(closing_query.qty, 0.0) as closing_qty,
        coalesce(vclosing_query.value, 0.0) as closing_value,
        coalesce(incoming_query.qty, 0.0) as incoming_qty,
        coalesce(vincoming_query.value, 0.0) as incoming_value,
        coalesce(outgoing_query.qty, 0.0) as outgoing_qty,
        coalesce(voutgoing_query.value, 0.0) as outgoing_value,
        coalesce(adjustment_in_query.qty, 0.0) as adjustment_in_qty,
        coalesce(vadjustment_in_query.value, 0.0) as adjustment_in_value,
        coalesce(adjustment_out_query.qty, 0.0) as adjustment_out_qty,
        coalesce(vadjustment_out_query.value, 0.0) as adjustment_out_value
    FROM
        product_product pp
        LEFT JOIN (select product_id, sum(qty) as qty
            from 
                ((select product_id, product_qty as qty from stock_move
                    where (location_id <> $P{location_id}) 
                    and (location_dest_id = $P{location_id})
                    and (state='done')
                    and (company_id=$P{company_id})
                    and ($P!{product_query})
                    and (date + INTERVAL '7 hours')::TIMESTAMP<($P{from_date}||' 00:00:01')::TIMESTAMP
                )
                union all
                (select product_id, -1*product_qty as qty from stock_move
                    where (location_id = $P{location_id}) 
                    and (location_dest_id <> $P{location_id})
                    and (state='done')
                    and (company_id=$P{company_id})
                    and ($P!{product_query})
                    and (date + INTERVAL '7 hours')::TIMESTAMP<($P{from_date}||' 00:00:01')::TIMESTAMP
                )
                ) sub
            group by product_id
            ) opening_query ON opening_query.product_id=pp.id
        LEFT JOIN (select product_id, sum(amt) as value
            from 
                ((select product_id, amount as amt from stock_move_value
                    where (location_id <> $P{location_id}) 
                    and (location_dest_id = $P{location_id})
                    and (company_id=$P{company_id})
                    and ($P!{product_query})
                    and (date + INTERVAL '7 hours')::TIMESTAMP<($P{from_date}||' 00:00:01')::TIMESTAMP
                )
                union all
                (select product_id, -1*amount as amt from stock_move_value
                    where (location_id = $P{location_id}) 
                    and (location_dest_id <> $P{location_id})
                    and (company_id=$P{company_id})
                    and ($P!{product_query})
                    and (date + INTERVAL '7 hours')::TIMESTAMP<($P{from_date}||' 00:00:01')::TIMESTAMP
                )
                ) sub
            group by product_id
            ) vopening_query ON vopening_query.product_id=pp.id
        LEFT JOIN (select product_id, sum(qty) as qty
            from 
                ((select product_id, product_qty as qty from stock_move
                    where (location_id <> $P{location_id}) 
                    and (location_dest_id = $P{location_id})
                    and (state='done')
                    and (company_id=$P{company_id})
                    and ($P!{product_query})
                    and (date + INTERVAL '7 hours')::TIMESTAMP<=($P{to_date} ||' 23:59:59')::TIMESTAMP
                )
                union all
                (select product_id, -1*product_qty as qty from stock_move
                    where (location_id = $P{location_id}) 
                    and (location_dest_id <> $P{location_id})
                    and (state='done')
                    and (company_id=$P{company_id})
                    and ($P!{product_query})
                    and (date + INTERVAL '7 hours')::TIMESTAMP<=($P{to_date} ||' 23:59:59')::TIMESTAMP
                )
                ) sub
            group by product_id
            ) closing_query ON closing_query.product_id=pp.id
        LEFT JOIN (select product_id, sum(amt) as value
            from 
                ((select product_id, amount as amt from stock_move_value
                    where (location_id <> $P{location_id}) 
                    and (location_dest_id = $P{location_id})
                    and (company_id=$P{company_id})
                    and ($P!{product_query})
                    and (date + INTERVAL '7 hours')::TIMESTAMP<=($P{to_date} ||' 23:59:59')::TIMESTAMP
                )
                union all
                (select product_id, -1*amount as amt from stock_move_value
                    where (location_id = $P{location_id}) 
                    and (location_dest_id <> $P{location_id})
                    and (company_id=$P{company_id})
                    and ($P!{product_query})
                    and (date + INTERVAL '7 hours')::TIMESTAMP<=($P{to_date} ||' 23:59:59')::TIMESTAMP
                )
                ) sub
            group by product_id
            ) vclosing_query ON vclosing_query.product_id=pp.id
        LEFT JOIN (select
                product_id, sum(product_qty) as qty
            from stock_move
            where (location_dest_id = $P{location_id})
                and (location_id=any(select id from stock_location where (usage!='inventory') and (id <> $P{location_id}))) 
                and (state='done')
                and (company_id=$P{company_id})
                and ($P!{product_query})
                and (date + INTERVAL '7 hours')::TIMESTAMP between ($P{from_date}||' 00:00:01')::TIMESTAMP and ($P{to_date} ||' 23:59:59')::TIMESTAMP
            group by product_id) incoming_query ON incoming_query.product_id=pp.id
        LEFT JOIN (select
                product_id, sum(amount) as value
            from stock_move_value
            where (location_dest_id = $P{location_id})
                and (location_id=any(select id from stock_location where (usage!='inventory') and (id <> $P{location_id}))) 
                and (company_id=$P{company_id})
                and (move_id is not NULL or move_id>0)
                and ($P!{product_query})
                and (date + INTERVAL '7 hours')::TIMESTAMP between ($P{from_date}||' 00:00:01')::TIMESTAMP and ($P{to_date} ||' 23:59:59')::TIMESTAMP
            group by product_id) vincoming_query ON vincoming_query.product_id=pp.id
        LEFT JOIN (select
                product_id, sum(product_qty) as qty
            from stock_move
            where (location_id = $P{location_id})
                and (location_dest_id=any(select id from stock_location where (usage!='inventory') and (id <> $P{location_id}))) 
                and (state='done')
                and (company_id=$P{company_id})
                and ($P!{product_query})
                and (date + INTERVAL '7 hours')::TIMESTAMP between ($P{from_date}||' 00:00:01')::TIMESTAMP and ($P{to_date} ||' 23:59:59')::TIMESTAMP
            group by product_id) outgoing_query ON outgoing_query.product_id=pp.id
        LEFT JOIN (select
                product_id, sum(amount) as value
            from stock_move_value
            where (location_id = $P{location_id})
                and (location_dest_id=any(select id from stock_location where (usage!='inventory') and (id <> $P{location_id}))) 
                and (company_id=$P{company_id})
                and (move_id is not NULL or move_id>0)
                and ($P!{product_query})
                and (date + INTERVAL '7 hours')::TIMESTAMP between ($P{from_date}||' 00:00:01')::TIMESTAMP and ($P{to_date} ||' 23:59:59')::TIMESTAMP
            group by product_id) voutgoing_query ON voutgoing_query.product_id=pp.id
        LEFT JOIN (select
                product_id, sum(product_qty) as qty
            from stock_move
            where (location_dest_id = $P{location_id})
                and (location_id=any(select id from stock_location where usage='inventory')) 
                and (state='done')
                and (company_id=$P{company_id})
                and ($P!{product_query})
                and (date + INTERVAL '7 hours')::TIMESTAMP between ($P{from_date}||' 00:00:01')::TIMESTAMP and ($P{to_date} ||' 23:59:59')::TIMESTAMP
            group by product_id) adjustment_in_query ON adjustment_in_query.product_id=pp.id
        LEFT JOIN (select
                product_id, sum(amount) as value
            from stock_move_value
            where (location_dest_id = $P{location_id})
                and (location_id=any(select id from stock_location where usage='inventory')) 
                and (company_id=$P{company_id})
                and (move_id is not NULL or move_id>0)
                and ($P!{product_query})
                and (date + INTERVAL '7 hours')::TIMESTAMP between ($P{from_date}||' 00:00:01')::TIMESTAMP and ($P{to_date} ||' 23:59:59')::TIMESTAMP
            group by product_id) vadjustment_in_query ON vadjustment_in_query.product_id=pp.id
        LEFT JOIN (select
                product_id, sum(product_qty) as qty
            from stock_move
            where (location_id = $P{location_id})
                and (location_dest_id=any(select id from stock_location where usage='inventory')) 
                and (state='done')
                and (company_id=$P{company_id})
                and ($P!{product_query})
                and (date + INTERVAL '7 hours')::TIMESTAMP between ($P{from_date}||' 00:00:01')::TIMESTAMP and ($P{to_date} ||' 23:59:59')::TIMESTAMP
            group by product_id) adjustment_out_query ON adjustment_out_query.product_id=pp.id
        LEFT JOIN (select
                product_id, sum(amount) as value
            from stock_move_value
            where (location_id = $P{location_id})
                and (location_dest_id=any(select id from stock_location where usage='inventory')) 
                and (company_id=$P{company_id})
                and (move_id is not NULL or move_id>0)
                and ($P!{product_query})
                and (date + INTERVAL '7 hours')::TIMESTAMP between ($P{from_date}||' 00:00:01')::TIMESTAMP and ($P{to_date} ||' 23:59:59')::TIMESTAMP
            group by product_id) vadjustment_out_query ON vadjustment_out_query.product_id=pp.id
    WHERE 
        (coalesce(opening_query.qty, 0.0)+coalesce(incoming_query.qty, 0.0)+coalesce(outgoing_query.qty, 0.0)+coalesce(adjustment_in_query.qty, 0.0)+coalesce(adjustment_out_query.qty,0.0)+coalesce(adjustment_out_query.qty,0.0))>0
        or 
        (coalesce(vopening_query.value, 0.0)+coalesce(vincoming_query.value, 0.0)+coalesce(voutgoing_query.value, 0.0)+coalesce(vadjustment_in_query.value, 0.0)+coalesce(vadjustment_out_query.value,0.0)+coalesce(vadjustment_out_query.value,0.0))>0
        ) sub
    INNER JOIN product_product item ON item.id=sub.product_id
    INNER JOIN product_template itemtmpl ON itemtmpl.id=item.product_tmpl_id
    INNER JOIN product_uom uom ON uom.id=itemtmpl.uom_id
WHERE itemtmpl.type='product']]>
	</queryString>
