	<queryString language="SQL">
		<![CDATA[SELECT
	aa.internal_type as account_type, 
	aa.code as account_code, aa.name as account_name,
	aj.name as journal_name, rp.name as partner_name, 
	aid.number as invoice_number, 
	aid.date_invoice as invoice_date,
	aid.date_invoice as date_due,
	coalesce(amlccy.name, compccy.name) as currency,
	aml.balance as original_base_amt,
	(case when coalesce(aml.currency_id,0)<>0 then aml.amount_currency else 0 end) as original_amt, 
	coalesce(payment.payment_amt, 0.0) as adjusted_base_amt, 
	coalesce(payment.payment_amt_ccy, 0.0) as adjusted_amt,
	(aml.balance - coalesce(payment.payment_amt, 0.0)) as unadjusted_base_amt,
	(aml.amount_currency - coalesce(payment.payment_amt_ccy, 0.0)) as unadjusted_amt
FROM
	account_move_line aml
	INNER JOIN account_move am ON am.id=aml.move_id
	INNER JOIN res_partner rp ON rp.id=aml.partner_id
	INNER JOIN account_account aa ON aa.id=aml.account_id
	INNER JOIN account_journal aj ON aj.id=aml.journal_id
	LEFT JOIN res_currency amlccy ON amlccy.id=aml.currency_id
	INNER JOIN res_company comp ON comp.id=aml.company_id
	INNER JOIN res_currency compccy ON compccy.id=comp.currency_id
	INNER JOIN account_invoice_advance_line aidl ON aidl.move_line_id=aml.id
	INNER JOIN account_invoice_advance aid ON aid.id=aidl.invoice_id
	LEFT JOIN (SELECT move_line_id, sum(payment_amt) as payment_amt, sum(payment_amt_ccy) as payment_amt_ccy
		FROM 
			((SELECT apr1.credit_move_id as move_line_id, 
				sum(-1*coalesce(apr1.amount,0.0)) as payment_amt,
				sum(-1*coalesce(apr1.amount_currency,0.0)) as payment_amt_ccy
			FROM account_partial_reconcile apr1 
				INNER JOIN account_move_line link_aml1 on link_aml1.id=apr1.debit_move_id
			WHERE link_aml1.date<=$P{as_of_date}::date::date
			GROUP BY apr1.credit_move_id)
			UNION ALL
			(SELECT apr2.debit_move_id as move_line_id, 
				sum(coalesce(apr2.amount,0.0)) as payment_amt,
				sum(coalesce(apr2.amount_currency,0.0)) as payment_amt_ccy
			FROM account_partial_reconcile apr2 
				INNER JOIN account_move_line link_aml2 on link_aml2.id=apr2.credit_move_id
			WHERE link_aml2.date<=$P{as_of_date}::date::date
			GROUP BY apr2.debit_move_id)) sub
		GROUP BY move_line_id) payment ON payment.move_line_id=aml.id
WHERE aid.type=$P{advance_type} and aid.date_invoice<=$P{as_of_date}::date::date
	and round(aml.balance - coalesce(payment.payment_amt, 0.0), 2)<> 0
ORDER BY aa.code, rp.name, aid.date_invoice, aid.number]]>
	</queryString>
