	<queryString language="SQL">
		<![CDATA[SELECT 1 AS order_no
, NULL AS gen_number
, ail.id AS line_id
, ai.id AS invoice_id
, ai.type AS invoice_type
, ai.number AS invoice_number
, partner.name AS partner_name
, ((COALESCE(partner.street,'')) || (COALESCE(', '||partner.street2,''))) AS partner_street
, ((COALESCE(partner.city,'')) || (COALESCE(', '||partner.zip,''))) AS partner_city
, partner.vat AS partner_npwp
, partner_ship.name AS cust_name_ship
, ((COALESCE(partner_ship.street,'')) || (COALESCE(', '||partner_ship.street2,''))) AS street_ship
, ((COALESCE(partner_ship.city,'')) || (COALESCE(', '||partner_ship.zip,''))) AS city_ship
, partner_ship.vat AS partner_ship_vat
, ai.date_invoice AS date_invoice
, COALESCE(ai.name, ai.reference) AS reference
, ai.origin AS origin
, apt.name AS payment_term
, apt.note AS payment_term_note
, '' AS product_code
, '' AS product_name
, trim(substr(ail."name", position(']' in ail."name")+1)) AS description
, ail.quantity AS qty
, '' AS uom_name
, ail.price_unit AS price_unit
, 0 AS discount
, ail.price_subtotal AS price_subtotal
, 0 AS amount_discount_subtotal
, ail.price_unit * ail.quantity AS price_subtotal_real
, (SELECT SUM(price_unit * quantity) 
			FROM account_invoice_advance_line WHERE invoice_id = ai.id) AS total_real
, 0 AS amount_discount
, 0 AS amount_downpayment
, ai.amount_untaxed AS amount_untaxed
, ai.amount_tax AS amount_tax
, ai.amount_total AS amount_total
, rcur.name AS mata_uang
, rcur.symbol AS symbol
, ai.comment AS comment
, ai.date_due AS due_date
, company.id AS company_id
, company.name AS company_name
, company.partner_office_id AS company_head_office
, ai.partner_bank_id AS partner_bank_id
, replace(rpbank.acc_number, concat('(',rcur.name,')'), '') AS bank_number
, bank.name AS bank_name
, bank.bank_branch AS bank_branch
, partner_bank.name AS bank_holder
FROM account_invoice_advance ai
LEFT OUTER JOIN account_invoice_advance_line ail on ai.id=ail.invoice_id
LEFT OUTER JOIN res_currency rcur ON rcur.id = ai.currency_id
LEFT OUTER JOIN res_partner partner ON partner.id = ai.partner_id
LEFT OUTER JOIN res_partner partner_ship ON partner_ship.id = ai.partner_id
LEFT OUTER JOIN res_country rc ON rc.id = partner.country_id
LEFT OUTER JOIN res_country_state rcs ON rcs."id" = partner.state_id
LEFT OUTER JOIN account_payment_term apt ON apt.id = ai.payment_term_id
LEFT OUTER JOIN res_company company ON company.id = ai.company_id
LEFT OUTER JOIN res_partner_bank rpbank ON rpbank.id = ai.partner_bank_id
LEFT OUTER JOIN res_bank bank ON bank.id = rpbank.bank_id
LEFT OUTER JOIN res_partner partner_bank ON partner_bank.id = rpbank.partner_id
WHERE ai.id= $P{id} -->>change here<<

UNION ALL

SELECT 2 AS order_no
, generate_series(1001, 1030 - (select count(*) from purchase_order_line pol where pol.order_id = $P{id})) AS gen_number -->>change here<<
, NULL AS line_id
, NULL AS invoice_id
, NULL AS invoice_type
, NULL AS invoice_number
, NULL AS partner_name
, NULL AS partner_street
, NULL AS partner_city
, NULL AS partner_npwp
, NULL AS cust_name_ship
, NULL AS street_ship
, NULL AS city_ship
, NULL AS partner_ship_vat
, NULL AS date_invoice
, NULL AS reference
, NULL AS origin
, NULL AS payment_term
, NULL AS payment_term_note
, NULL AS product_code
, NULL AS product_name
, NULL AS description
, NULL AS qty
, NULL AS uom_name
, NULL AS price_unit
, NULL AS discount
, NULL AS price_subtotal
, NULL AS amount_discount_subtotal
, NULL AS price_subtotal_real
, (SELECT SUM(price_unit * quantity) 
			FROM account_invoice_advance_line WHERE invoice_id = $P{id}) AS total_real
, 0 AS amount_discount
, 0 AS amount_downpayment
, (SELECT amount_untaxed FROM account_invoice_advance WHERE id = $P{id}) AS amount_untaxed
, (SELECT amount_tax FROM account_invoice_advance WHERE id = $P{id}) AS amount_tax
, (SELECT amount_total FROM account_invoice_advance WHERE id = $P{id}) AS amount_total
, (SELECT currency.name FROM account_invoice_advance ai
		LEFT OUTER JOIN res_currency currency ON currency.id = ai.currency_id
		WHERE ai.id = $P{id}) AS mata_uang
, (SELECT currency.symbol FROM account_invoice_advance ai
		LEFT OUTER JOIN res_currency currency ON currency.id = ai.currency_id
		WHERE ai.id = $P{id}) AS symbol
, (SELECT comment FROM account_invoice_advance WHERE id = $P{id}) AS comment
, NULL AS due_date
, NULL AS company_id
, NULL AS company_name
, NULL AS company_head_office
, (SELECT partner_bank_id FROM account_invoice_advance WHERE id = $P{id}) AS partner_bank_id
, (SELECT replace(rpbank.acc_number, concat('(',rcur.name,')'), '') FROM account_invoice_advance ai 
		LEFT OUTER JOIN res_currency rcur ON rcur.id = ai.currency_id
		LEFT OUTER JOIN res_partner_bank rpbank ON rpbank.id = ai.partner_bank_id
		LEFT OUTER JOIN res_bank bank ON bank.id = rpbank.bank_id
		LEFT OUTER JOIN res_partner partner_bank ON partner_bank.id = rpbank.partner_id
		WHERE ai.id = $P{id}) AS bank_number
, (SELECT bank.name FROM account_invoice_advance ai 
		LEFT OUTER JOIN res_currency rcur ON rcur.id = ai.currency_id
		LEFT OUTER JOIN res_partner_bank rpbank ON rpbank.id = ai.partner_bank_id
		LEFT OUTER JOIN res_bank bank ON bank.id = rpbank.bank_id
		LEFT OUTER JOIN res_partner partner_bank ON partner_bank.id = rpbank.partner_id
		WHERE ai.id = $P{id}) AS bank_name
, (SELECT bank.bank_branch FROM account_invoice_advance ai 
		LEFT OUTER JOIN res_currency rcur ON rcur.id = ai.currency_id
		LEFT OUTER JOIN res_partner_bank rpbank ON rpbank.id = ai.partner_bank_id
		LEFT OUTER JOIN res_bank bank ON bank.id = rpbank.bank_id
		LEFT OUTER JOIN res_partner partner_bank ON partner_bank.id = rpbank.partner_id
		WHERE ai.id = $P{id}) AS bank_branch
, (SELECT partner_bank.name FROM account_invoice_advance ai 
		LEFT OUTER JOIN res_currency rcur ON rcur.id = ai.currency_id
		LEFT OUTER JOIN res_partner_bank rpbank ON rpbank.id = ai.partner_bank_id
		LEFT OUTER JOIN res_bank bank ON bank.id = rpbank.bank_id
		LEFT OUTER JOIN res_partner partner_bank ON partner_bank.id = rpbank.partner_id
		WHERE ai.id = $P{id}) AS bank_holder
ORDER BY order_no, line_id ASC]]>
	</queryString>
