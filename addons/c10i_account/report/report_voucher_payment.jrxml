	<parameter name="id" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[13]]></defaultValueExpression>
	</parameter>
	<parameter name="currency_name" class="java.lang.String">
		<defaultValueExpression><![CDATA[13]]></defaultValueExpression>
	</parameter>
	<queryString language="SQL">
		<![CDATA[SELECT * FROM
((SELECT
	(case ap.payment_type when 'inbound' then 0 else 3 end) as order_line, 
	ap.name as voucher_number, ap.payment_date as date, 
	date_part('day', ap.payment_date) AS date_day, 
 	date_part('month', ap.payment_date) AS date_month, 
	date_part('year', ap.payment_date) AS date_year, 
	(CASE WHEN ap.payment_type = 'inbound' THEN 'sale'
		WHEN ap.payment_type = 'outbound' THEN 'purchase'
		WHEN ap.payment_type = 'transfer' THEN 'transfer'
		ELSE 'sale'
		END) as voucher_type, 
	aj.type as journal_type,
	aj.name AS jorunal_name,
	rp.name as partner_name, 
	(case ap.payment_type when 'inbound' then adr.code else acr.code end) as coa, 
	(case ap.payment_type when 'inbound' then adr.name else acr.name end) as coa_name, 
	coalesce(ap.communication,ap.name) as description, 
	ap.amount, 
	(case ap.payment_type 
		when 'inbound' then (case when ap.amount>0 then ap.amount else 0 end)
		when 'outbound' then (case when (ap.amount-ap.amount_charges)>0 then 0 else -1*(ap.amount-ap.amount_charges) end)
		when 'transfer' then (case when (ap.amount-ap.amount_charges)>0 then 0 else -1*(ap.amount-ap.amount_charges) end)
		else 0.0 end) as debit, 
	(case ap.payment_type 
		when 'inbound' then (case when ap.amount>0 then 0 else -1*ap.amount end)
		when 'outbound' then (case when (ap.amount-ap.amount_charges)>0 then (ap.amount-ap.amount_charges) else 0 end)
		when 'transfer' then (case when (ap.amount-ap.amount_charges)>0 then (ap.amount-ap.amount_charges) else 0 end)
		else 0.0 end) as credit, 
	ap.company_id, 
	rcomp.name as company_name, 
	rc.name as currency, 
	ap.notes as note
	, bank.name AS bank_name
	, replace(rpbank.acc_number, concat('(',rc.name,')'), '')  AS acc_num
	, ap.check_number AS check_number
	, ap.check_date AS check_date
FROM
	account_payment ap
	INNER JOIN account_journal aj ON aj.id=ap.journal_id
	INNER JOIN account_account adr ON adr.id=aj.default_debit_account_id
	INNER JOIN account_account acr ON acr.id=aj.default_credit_account_id
	LEFT JOIN res_partner rp ON rp.id=ap.partner_id
	INNER JOIN res_currency rc ON rc.id=ap.currency_id
	INNER JOIN res_company rcomp ON rcomp.id=ap.company_id
	LEFT OUTER JOIN res_partner_bank rpbank on rpbank.id = aj.bank_account_id
	LEFT OUTER JOIN res_bank bank on bank.id = rpbank.bank_id
WHERE ap.id=$P{id})
UNION ALL
(SELECT
	(case ap.payment_type when 'inbound' then 3 else 0 end) as order_line, 
	ap.name as voucher_number, 
	ap.payment_date as date, 
	date_part('day', ap.payment_date) AS date_day,
	date_part('month', ap.payment_date) AS date_month,
	date_part('year', ap.payment_date) AS date_year, 
	(CASE WHEN ap.payment_type = 'inbound' THEN 'sale'
			WHEN ap.payment_type = 'outbound' THEN 'purchase'
			WHEN ap.payment_type = 'transfer' THEN 'transfer'
			ELSE 'sale'
			END) as voucher_type,
	aj.type as journal_type,
	aj.name AS jorunal_name,	
	rp.name as partner_name, 
	(case ap.payment_type when 'inbound' then aar.code when 'transfer' then aao.code else aap.code end) as coa, 
	(case ap.payment_type when 'inbound' then aar.name when 'transfer' then aao.name else aap.name end) as coa_name, 
	coalesce(ap.communication,ap.name) as description, 
	ap.amount, 
	(case ap.payment_type 
		when 'inbound' then (case when ap.amount>0 then 0 else -1*ap.amount end)
		when 'outbound' then (case when ap.amount>0 then ap.amount else 0 end)
		when 'transfer' then (case when ap.amount>0 then ap.amount else 0 end)
		else 0.0 end) as debit, 
	(case ap.payment_type 
		when 'inbound' then (case when ap.amount>0 then ap.amount else 0 end)
		when 'outbound' then (case when ap.amount>0 then 0 else -1*ap.amount end)
		when 'transfer' then (case when ap.amount>0 then 0 else -1*ap.amount end)
		else 0.0 end) as credit, 
	ap.company_id, rcomp.name as company_name, 
	rc.name as currency, 
	ap.notes as note
	, bank.name AS bank_name
	, replace(rpbank.acc_number, concat('(',rc.name,')'), '')  AS acc_num
	, ap.check_number AS check_number
	, ap.check_date AS check_date
FROM
	account_payment ap
	INNER JOIN account_journal aj ON aj.id=ap.journal_id
	INNER JOIN account_journal dest_aj ON dest_aj.id=ap.destination_journal_id
	LEFT JOIN res_partner rp ON rp.id=ap.partner_id
	LEFT JOIN (SELECT 
			split_part(res_id,',',2)::int as partner_id, split_part(value_reference,',',2)::int as account_id
		FROM ir_property 
		WHERE name='property_account_receivable_id' and split_part(value_reference,',',1)='account.account') prop_aar ON prop_aar.partner_id=rp.id
	LEFT JOIN account_account aar ON aar.id=prop_aar.account_id
	LEFT JOIN (SELECT 
			split_part(res_id,',',2)::int as partner_id, split_part(value_reference,',',2)::int as account_id
		FROM ir_property 
		WHERE name='property_account_payable_id' and split_part(value_reference,',',1)='account.account') prop_aap ON prop_aap.partner_id=rp.id
	LEFT JOIN account_account aap ON aap.id=prop_aap.account_id
	LEFT JOIN account_account aao ON aao.id=dest_aj.default_debit_account_id
	INNER JOIN res_currency rc ON rc.id=ap.currency_id
	INNER JOIN res_company rcomp ON rcomp.id=ap.company_id
	LEFT OUTER JOIN res_partner_bank rpbank on rpbank.id = aj.bank_account_id
	LEFT OUTER JOIN res_bank bank on bank.id = rpbank.bank_id
WHERE ap.id=$P{id} and (select count(id) from account_payment_line where payment_id=$P{id})=0)
UNION ALL
(SELECT
	1 as order_line, 
	ap.name as voucher_number, ap.payment_date as date, 
	date_part('day', ap.payment_date) AS date_day, 
	date_part('month', ap.payment_date) AS date_month, 
	date_part('year', ap.payment_date) AS date_year, 
	(CASE WHEN ap.payment_type = 'inbound' THEN 'sale'
		WHEN ap.payment_type = 'outbound' THEN 'purchase'
		WHEN ap.payment_type = 'transfer' THEN 'transfer'
		ELSE 'sale'
		END) as voucher_type, 
	aj.type as journal_type,
	aj.name AS jorunal_name,
	rp.name as partner_name, 
	aa.code as coa, aa.name as coa_name, 
	coalesce(ap.communication,apl.name) as description, 
	ap.amount, 
	(case ap.payment_type 
		when 'inbound' then (case when apl.amount_to_pay>0 then 0 when apl.amount_to_pay=0 then 0 else -1*apl.amount_to_pay + 
			(case when apl.writeoff_account_id is not NULL or coalesce(apl.writeoff_account_id,0)<>0 then 1*apl.payment_difference else 0 end) 
		end)
		when 'outbound' then (case when apl.amount_to_pay>0 then apl.amount_to_pay  + 
			(case when apl.writeoff_account_id is not NULL or coalesce(apl.writeoff_account_id,0)<>0 then 1*apl.payment_difference else 0 end) when apl.amount_to_pay=0 then 0 else 0 end)
		else 0.0 end) as debit, 
	(case ap.payment_type 
		when 'inbound' then (case 
				when apl.amount_to_pay>0 and apl.writeoff_account_id is not null then apl.amount_to_pay  + 1*apl.payment_difference 
				when apl.amount_to_pay>0 and apl.writeoff_account_id is null then apl.amount_to_pay when apl.amount_to_pay=0 then 0 else 0 end)
		when 'outbound' then (case when apl.amount_to_pay<0 then 0 when apl.amount_to_pay>0 then 0 when apl.amount_to_pay=0 then 0 else (CASE WHEN apl.writeoff_account_id is not null THEN -1*apl.amount_to_pay  + 1*apl.payment_difference
									WHEN apl.writeoff_account_id is not null THEN -1*apl.amount_to_pay end) end)
		else 0.0 end) as credit,  
	ap.company_id, 
	rcomp.name as company_name, 
	rc.name as currency, 
	ap.notes as note
	, bank.name AS bank_name
	, replace(rpbank.acc_number, concat('(',rc.name,')'), '')  AS acc_num
	, ap.check_number AS check_number
	, ap.check_date AS check_date
FROM
	account_payment ap
	INNER JOIN account_payment_line apl ON apl.payment_id=ap.id
	INNER JOIN account_move_line aml ON aml.id=apl.move_line_id
	LEFT JOIN account_invoice ai ON ai.id=apl.invoice_id
	INNER JOIN account_account aa ON aa.id=aml.account_id
	INNER JOIN account_journal aj ON aj.id=ap.journal_id
	LEFT JOIN res_partner rp ON rp.id=ap.partner_id
	INNER JOIN res_currency rc ON rc.id=ap.currency_id
	INNER JOIN res_company rcomp ON rcomp.id=ap.company_id
	LEFT OUTER JOIN res_partner_bank rpbank on rpbank.id = aj.bank_account_id
	LEFT OUTER JOIN res_bank bank on bank.id = rpbank.bank_id
WHERE ap.id=$P{id})
UNION ALL
(SELECT
	2 as order_line, 
	ap.name as voucher_number, 
	ap.payment_date as date, 
	date_part('day', ap.payment_date) AS date_day, 
	date_part('month', ap.payment_date) AS date_month, 
	date_part('year', ap.payment_date) AS date_year, 
	(CASE WHEN ap.payment_type = 'inbound' THEN 'sale'
			WHEN ap.payment_type = 'outbound' THEN 'purchase'
			WHEN ap.payment_type = 'transfer' THEN 'transfer'
			ELSE 'sale'
			END) as voucher_type, 
	aj.type as journal_type,
	aj.name AS jorunal_name,
	rp.name as partner_name, 
	aa.code as coa, 
	aa.name as coa_name, 
	(case when ai.id is not null then 'Write-Off Invoice '||ai.number 
		else 'Write-off ' end) as description, 
	ap.amount, 
	(case ap.payment_type 
		when 'inbound' then (case when apl.payment_difference>0 then apl.payment_difference else 0 end)
		when 'outbound' then (case when apl.payment_difference>0 then 0 else -1*apl.payment_difference end)
		else 0.0 end) as debit, 
	(case ap.payment_type 
		when 'inbound' then (case when apl.payment_difference>0 then 0 else -1*apl.payment_difference end)
		when 'outbound' then (case when apl.payment_difference>0 then apl.payment_difference else 0 end)
		else 0.0 end) as credit, 
	ap.company_id, 
	rcomp.name as company_name, 
	rc.name as currency, 
	ap.notes as note
	, bank.name AS bank_name
	, replace(rpbank.acc_number, concat('(',rc.name,')'), '')  AS acc_num
	, ap.check_number AS check_number
	, ap.check_date AS check_date
FROM
	account_payment ap
	INNER JOIN account_payment_line apl ON apl.payment_id=ap.id
	INNER JOIN account_move_line aml ON aml.id=apl.move_line_id
	LEFT JOIN account_invoice ai ON ai.id=apl.invoice_id
	INNER JOIN account_account aa ON aa.id=apl.writeoff_account_id
	INNER JOIN account_journal aj ON aj.id=ap.journal_id
	LEFT JOIN res_partner rp ON rp.id=ap.partner_id
	INNER JOIN res_currency rc ON rc.id=ap.currency_id
	INNER JOIN res_company rcomp ON rcomp.id=ap.company_id
	LEFT OUTER JOIN res_partner_bank rpbank on rpbank.id = aj.bank_account_id
	LEFT OUTER JOIN res_bank bank on bank.id = rpbank.bank_id
WHERE ap.id=$P{id})
UNION ALL
(SELECT
	4 as order_line, 
	ap.name as voucher_number, ap.payment_date as date, 
	date_part('day', ap.payment_date) AS date_day, 
 	date_part('month', ap.payment_date) AS date_month, 
	date_part('year', ap.payment_date) AS date_year, 
	(CASE WHEN ap.payment_type = 'inbound' THEN 'sale'
		WHEN ap.payment_type = 'outbound' THEN 'purchase'
		WHEN ap.payment_type = 'transfer' THEN 'transfer'
		ELSE 'sale'
		END) as voucher_type, 
	aj.type as journal_type,
	aj.name AS jorunal_name,
	rp.name as partner_name, 
	abc.code as coa, 
	abc.name as coa_name, 
	coalesce(ap.memo_charges,ap.name) as description, 
	ap.amount, 
	(CASE when ap.payment_type = 'transfer' THEN 0 else ap.amount_charges end) as debit, 
	(CASE when ap.payment_type = 'transfer' THEN ap.amount_charges else 0 end) as credit, 
	ap.company_id, 
	rcomp.name as company_name, 
	rc.name as currency, 
	ap.notes as note
	, bank.name AS bank_name
	, replace(rpbank.acc_number, concat('(',rc.name,')'), '')  AS acc_num
	, ap.check_number AS check_number
	, ap.check_date AS check_date
FROM
	account_payment ap
	INNER JOIN account_journal aj ON aj.id=ap.journal_id
	INNER JOIN account_account abc ON abc.id=ap.charge_account_id
	LEFT JOIN res_partner rp ON rp.id=ap.partner_id
	INNER JOIN res_currency rc ON rc.id=ap.currency_id
	INNER JOIN res_company rcomp ON rcomp.id=ap.company_id
	LEFT OUTER JOIN res_partner_bank rpbank on rpbank.id = aj.bank_account_id
	LEFT OUTER JOIN res_bank bank on bank.id = rpbank.bank_id
WHERE ap.id=$P{id} and ap.amount_charges>0)
UNION ALL
(SELECT
	5 as order_line, 
	ap.name as voucher_number, ap.payment_date as date, 
	date_part('day', ap.payment_date) AS date_day, 
 	date_part('month', ap.payment_date) AS date_month, 
	date_part('year', ap.payment_date) AS date_year, 
	(CASE WHEN ap.payment_type = 'inbound' THEN 'sale'
		WHEN ap.payment_type = 'outbound' THEN 'purchase'
		WHEN ap.payment_type = 'transfer' THEN 'transfer'
		ELSE 'sale'
		END) as voucher_type, 
	aj.type as journal_type,
	aj.name AS jorunal_name,
	rp.name as partner_name, 
	acr.code as coa, 
	acr.name as coa_name, 
	coalesce(ap.memo_charges,ap.name) as description, 
	ap.amount, 
	0 as debit, 
	ap.amount_charges as credit, 
	ap.company_id, 
	rcomp.name as company_name, 
	rc.name as currency, 
	ap.notes as note
  , bank.name AS bank_name
	, replace(rpbank.acc_number, concat('(',rc.name,')'), '')  AS acc_num
	, ap.check_number AS check_number
	, ap.check_date AS check_date
FROM
	account_payment ap
	INNER JOIN account_journal aj ON aj.id=ap.journal_id
	INNER JOIN account_account acr ON acr.id=aj.default_credit_account_id
	LEFT JOIN res_partner rp ON rp.id=ap.partner_id
	INNER JOIN res_currency rc ON rc.id=ap.currency_id
	INNER JOIN res_company rcomp ON rcomp.id=ap.company_id
	LEFT OUTER JOIN res_partner_bank rpbank on rpbank.id = aj.bank_account_id
	LEFT OUTER JOIN res_bank bank on bank.id = rpbank.bank_id
WHERE ap.id=$P{id} and ap.amount_charges>0 and ap.payment_type='outbound')) sub
ORDER BY order_line, description]]>
	</queryString>
