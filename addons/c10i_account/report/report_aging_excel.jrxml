						inner join account_partial_reconcile apr on apr.debit_move_id = inneraml.id
			      inner join account_move_line inneraml2 on inneraml2.id = apr.credit_move_id
						where apr.debit_move_id = aml.id and inneraml2.date <= $P{from_date}::date)) as base_amt_paid

						,aml.balance + ((select coalesce(sum(apr.amount),0) from account_move_line inneraml
						inner join account_partial_reconcile apr on apr.credit_move_id = inneraml.id
			      inner join account_move_line inneraml2 on inneraml2.id = apr.debit_move_id
						where apr.credit_move_id = aml.id and inneraml2.date <= $P{from_date}::date) -
						(select coalesce(sum(apr.amount),0) from account_move_line inneraml
						inner join account_partial_reconcile apr on apr.debit_move_id = inneraml.id
			      inner join account_move_line inneraml2 on inneraml2.id = apr.credit_move_id
						where apr.debit_move_id = aml.id and inneraml2.date <= $P{from_date}::date)) as base_outstanding_amount

						,((select coalesce(sum(apr.amount_currency),0) from account_move_line inneraml
						inner join account_partial_reconcile apr on apr.credit_move_id = inneraml.id
			      inner join account_move_line inneraml2 on inneraml2.id = apr.debit_move_id
						where apr.credit_move_id = aml.id and inneraml2.date <= $P{from_date}::date)
						-
						(select coalesce(sum(apr.amount_currency),0) from account_move_line inneraml
						inner join account_partial_reconcile apr on apr.debit_move_id = inneraml.id
			      inner join account_move_line inneraml2 on inneraml2.id = apr.credit_move_id
						where apr.debit_move_id = aml.id and inneraml2.date <= $P{from_date}::date)) as amt_paid

						,aml.amount_currency + ((select coalesce(sum(apr.amount_currency),0) from account_move_line inneraml
						inner join account_partial_reconcile apr on apr.credit_move_id = inneraml.id
			      inner join account_move_line inneraml2 on inneraml2.id = apr.debit_move_id
						where apr.credit_move_id = aml.id and inneraml2.date <= $P{from_date}::date) -
						(select coalesce(sum(apr.amount_currency),0) from account_move_line inneraml
						inner join account_partial_reconcile apr on apr.debit_move_id = inneraml.id
			      inner join account_move_line inneraml2 on inneraml2.id = apr.credit_move_id
						where apr.debit_move_id = aml.id and inneraml2.date <= $P{from_date}::date)) as outstanding_amount
, coalesce(rc."name",rcomp.name) currency
, NULL usia
from account_move_line aml
inner join account_account aa on aa.id = aml.account_id
inner join account_move am on am.id = aml.move_id
left outer join res_partner rp on rp.id = aml.partner_id
left outer join account_journal aj on aj.id = am.journal_id
left join account_invoice_advance aid on aid.move_id = aml.move_id
LEFT OUTER JOIN res_currency rc ON rc."id" = aml.currency_id
LEFT OUTER JOIN res_currency rcomp ON rcomp."id" = aml.company_currency_id
where aa.internal_type = $P{Partner_type} and am.state = 'posted'
and $P!{partner_id_query}
AND $P!{journal_query}
AND am.company_id = $P{company_id}
and aml.invoice_id is null
and aml.date <= $P{from_date}::date
and round(((select coalesce(sum(amount),0) from account_move_line inneraml
						inner join account_partial_reconcile apr on apr.debit_move_id = inneraml.id
						where apr.credit_move_id = aml.id) -
						(select coalesce(sum(amount),0) from account_move_line inneraml
						inner join account_partial_reconcile apr on apr.credit_move_id = inneraml.id
						where apr.debit_move_id = aml.id)) + aml.balance,6) <> 0
