            <field name="email">bms@bumimajusawit.co.id</field>
            <field name="street">Graha BIP Lt. 3 Zone E</field>
            <field name="street2">Jl. Jend. Gatot Subroto Kav. 23</field>
            <field name="zip">12930</field>
            <field name="vat"></field>
            <field name="city">Jakarta Selatan</field>
            <field name="state_id" ref="base.state_id_kb"/>
