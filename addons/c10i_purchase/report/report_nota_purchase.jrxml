	<queryString language="SQL">
		<![CDATA[SELECT 1 AS order_no
, NULL AS gen_number
, pol.id AS line_id
, company.id AS company_id
, company.name AS company_name
, company.partner_office_id AS company_head_office
, po.service_order AS product_type
, po.name AS purchase_name
, po.date_order AS purchase_date
, po.date_planned AS date_planned
, po.partner_ref AS reference
, (select string_agg(pr.name,',') 
   from purchase_order_purchase_request_rel poprl 
	 LEFT JOIN purchase_request pr ON pr.id = poprl.purchase_request_id 
	 where poprl.purchase_order_id = po.id) as purchase_request
, apt.name AS payment_name
, si.name AS incoterm_name
, vendor.name AS vendor_name
, vendor.street AS vendor_street
, vendor.street2 AS vendor_street2
, vendor.city AS vendor_city
, vendor.vat AS vendor_vat
, vendor.phone AS vendor_phone
, vendor_attn.name AS vendor_attn_name
, vendor_attn.phone AS vendor_attn_phone
, shipping.name AS shipping_name
, shipping.street AS shipping_street
, shipping.street2 AS shipping_street2
, shipping.city AS shipping_city
, shipping.vat AS shipping_vat
, shipping.phone AS shipping_phone
, shipping.mobile AS shipping_mobile
, shipping_parent.name AS shipping_parent_name
, invoice.name AS invoice_name
, invoice.street AS invoice_street
, invoice.street2 AS invoice_street2
, invoice.city AS invoice_city
, invoice.vat AS invoice_vat
, invoice.phone AS invoice_phone
, invoice.mobile AS invoice_mobile
, invoice.fax AS invoice_fax
, invoice_parent.name AS invoice_parent_name
, pt.default_code AS product_code
, pt.name AS product_name
, (CASE WHEN pol.name ilike '['||pt.default_code||'] '||pt.name THEN NULL ELSE pol.name END) AS line_description
, pol.product_qty AS qty
, pu.name AS uom_name
, pol.purchase_qty AS purchase_qty
, pu1.name AS purchase_uom
, pol.price_unit AS price_unit
, pol.price_subtotal AS price_subtotal
, po.amount_untaxed AS amount_untaxed
, 0 AS amount_discount
, po.amount_tax AS amount_tax
, po.amount_total AS amount_total
, po.notes AS notes
, curr.name AS currency_name
, curr.symbol AS currency_symbol
, po.report_sign_1 AS report_sign_1
, po.report_sign_2 AS report_sign_2
, po.report_sign_3 AS report_sign_3
, po.report_sign_4 AS report_sign_4
, po.report_sign_5 AS report_sign_5
, po.report_sign_6 AS report_sign_6
FROM purchase_order_line pol
INNER JOIN purchase_order po ON po.id = pol.order_id
LEFT OUTER JOIN res_company company ON company.id = po.company_id
LEFT OUTER JOIN account_payment_term apt ON apt.id = po.payment_term_id
LEFT OUTER JOIN stock_incoterms si ON si.id = po.incoterm_id
LEFT OUTER JOIN res_partner vendor ON vendor.id = po.partner_id
LEFT OUTER JOIN res_partner vendor_attn ON vendor_attn.id = (SELECT id FROM res_partner where parent_id = po.partner_id and type = 'contact' limit 1)
LEFT OUTER JOIN res_partner shipping ON shipping.id = po.shipping_partner_id
LEFT OUTER JOIN res_partner shipping_parent ON shipping_parent.id = shipping.parent_id
LEFT OUTER JOIN res_partner invoice ON invoice.id = po.invoice_partner_id
LEFT OUTER JOIN res_partner invoice_parent ON invoice_parent.id = invoice.parent_id
LEFT OUTER JOIN product_product pp ON pp.id = pol.product_id
LEFT OUTER JOIN product_uom pu ON pu.id = pol.product_uom
LEFT OUTER JOIN product_uom pu1 ON pu1.id = pol.purchase_unit
LEFT OUTER JOIN product_template pt ON pt."id" = pp.product_tmpl_id
LEFT OUTER JOIN res_currency curr ON curr."id" = po.currency_id
WHERE po.id = $P{id}

UNION ALL

SELECT 2 AS order_no
, generate_series(1001, 1023 - (select count(*) from purchase_order_line pol where pol.order_id = $P{id}))  AS gen_number
, NULL AS line_id
, NULL AS company_id
, NULL AS company_name
, NULL AS company_head_office
, NULL AS product_type
, NULL AS purchase_name
, NULL AS purchase_date
, NULL AS date_planned
, NULL AS reference
, NULL AS purchase_request
, NULL AS payment_name
, NULL AS incoterm_name
, NULL AS vendor_name
, NULL AS vendor_street
, NULL AS vendor_street2
, NULL AS vendor_city
, NULL AS vendor_vat
, NULL AS vendor_phone
, NULL AS vendor_attn_name
, NULL AS vendor_attn_phone
, (SELECT shipping.name FROM purchase_order po
			LEFT OUTER JOIN res_partner shipping ON shipping.id = po.shipping_partner_id
			WHERE po.id = $P{id}
			) AS shipping_name
, (SELECT shipping.street FROM purchase_order po
			LEFT OUTER JOIN res_partner shipping ON shipping.id = po.shipping_partner_id
			WHERE po.id = $P{id}
			) AS shipping_street
, (SELECT shipping.street2 FROM purchase_order po
			LEFT OUTER JOIN res_partner shipping ON shipping.id = po.shipping_partner_id
			WHERE po.id = $P{id}
			) AS shipping_street2
, (SELECT shipping.city FROM purchase_order po
			LEFT OUTER JOIN res_partner shipping ON shipping.id = po.shipping_partner_id
			WHERE po.id = $P{id}
			) AS shipping_city
, (SELECT shipping.vat FROM purchase_order po
			LEFT OUTER JOIN res_partner shipping ON shipping.id = po.shipping_partner_id
			WHERE po.id = $P{id}
			) AS shipping_vat
, (SELECT shipping.phone FROM purchase_order po
			LEFT OUTER JOIN res_partner shipping ON shipping.id = po.shipping_partner_id
			WHERE po.id = $P{id}
			) AS shipping_phone
, (SELECT shipping.mobile FROM purchase_order po
			LEFT OUTER JOIN res_partner shipping ON shipping.id = po.shipping_partner_id
			WHERE po.id = $P{id}
			) AS shipping_mobile
, (SELECT shipping_parent.name FROM purchase_order po
			LEFT OUTER JOIN res_partner shipping ON shipping.id = po.shipping_partner_id
			LEFT OUTER JOIN res_partner shipping_parent ON shipping_parent.id = shipping.parent_id
			WHERE po.id = $P{id}
			) AS shipping_parent_name
, (SELECT invoice.name FROM purchase_order po
			LEFT OUTER JOIN res_partner invoice ON invoice.id = po.invoice_partner_id
			WHERE po.id = $P{id}
			) AS invoice_name
, (SELECT invoice.street FROM purchase_order po
			LEFT OUTER JOIN res_partner invoice ON invoice.id = po.invoice_partner_id
			WHERE po.id = $P{id}
			) AS invoice_street
, (SELECT invoice.street2 FROM purchase_order po
			LEFT OUTER JOIN res_partner invoice ON invoice.id = po.invoice_partner_id
			WHERE po.id = $P{id}
			) AS invoice_street2
, (SELECT invoice.city FROM purchase_order po
			LEFT OUTER JOIN res_partner invoice ON invoice.id = po.invoice_partner_id
			WHERE po.id = $P{id}
			) AS invoice_city
, (SELECT invoice.vat FROM purchase_order po
			LEFT OUTER JOIN res_partner invoice ON invoice.id = po.invoice_partner_id
			WHERE po.id = $P{id}
			) AS invoice_vat
, (SELECT invoice.phone FROM purchase_order po
			LEFT OUTER JOIN res_partner invoice ON invoice.id = po.invoice_partner_id
			WHERE po.id = $P{id}
			) AS invoice_phone
, (SELECT invoice.mobile FROM purchase_order po
			LEFT OUTER JOIN res_partner invoice ON invoice.id = po.invoice_partner_id
			WHERE po.id = $P{id}
			) AS invoice_mobile
, (SELECT invoice.fax FROM purchase_order po
			LEFT OUTER JOIN res_partner invoice ON invoice.id = po.invoice_partner_id
			WHERE po.id = $P{id}
			) AS invoice_fax
, (SELECT invoice_parent.name FROM purchase_order po
			LEFT OUTER JOIN res_partner invoice ON invoice.id = po.invoice_partner_id
			LEFT OUTER JOIN res_partner invoice_parent ON invoice_parent.id = invoice.parent_id
			WHERE po.id = $P{id}
			) AS invoice_parent_name
, NULL AS product_code
, NULL AS product_name
, NULL AS line_description
, NULL AS qty
, NULL AS uom_name
, NULL AS purchase_qty
, NULL AS purchase_uom
, NULL AS price_unit
, NULL AS price_subtotal
, (SELECT amount_untaxed FROM purchase_order WHERE id = $P{id}) AS amount_untaxed
, 0 AS amount_discount
, (SELECT amount_tax FROM purchase_order WHERE id = $P{id}) AS amount_tax
, (SELECT amount_total FROM purchase_order WHERE id = $P{id}) AS amount_total
, (SELECT notes FROM purchase_order WHERE id = $P{id}) AS notes
, (SELECT curr.name FROM purchase_order po 
		LEFT OUTER JOIN res_currency curr ON curr."id" = po.currency_id
		WHERE po.id = $P{id}
		) AS currency_name
, (SELECT curr.symbol FROM purchase_order po 
		LEFT OUTER JOIN res_currency curr ON curr."id" = po.currency_id
		WHERE po.id = $P{id}
		) AS currency_symbol
, (SELECT report_sign_1 FROM purchase_order WHERE id = $P{id}) AS report_sign_1
, (SELECT report_sign_2 FROM purchase_order WHERE id = $P{id}) AS report_sign_2
, (SELECT report_sign_3 FROM purchase_order WHERE id = $P{id}) AS report_sign_3
, (SELECT report_sign_4 FROM purchase_order WHERE id = $P{id}) AS report_sign_4
, (SELECT report_sign_5 FROM purchase_order WHERE id = $P{id}) AS report_sign_5
, (SELECT report_sign_6 FROM purchase_order WHERE id = $P{id}) AS report_sign_6
ORDER BY order_no, line_id]]>
	</queryString>
